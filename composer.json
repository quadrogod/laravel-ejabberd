{
  "name": "quadrogod/laravel-ejabberd",
  "description": "Ejabbered API Wrapper for Laravel/lumen",
  "license": "MIT",
  "authors": [
    {
      "name": "kevin-kibet",
      "email": "kevynkybet@gmail.com"
    },
    {
      "name": "Alex Molchanov",
      "email": "quadrogod@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Ejabberd\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ejabberd\\Providers\\EjabberdServiceProvider"
      ],
      "aliases": {
        "Ejabberd": "Ejabberd\\Facades\\Ejabberd"
      }
    }
  },
  "minimum-stability": "stable"
}
